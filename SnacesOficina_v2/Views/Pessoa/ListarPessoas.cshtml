@model IEnumerable<SnacesOficina_v2.Models.ModeloPessoa>
<link href="~/Content/style.css" rel="stylesheet" />
@*<link href="~/Content/style - Cópia .css" rel="stylesheet" />*@
@{
    ViewBag.Title = "ListarPessoas";
}
<ul>
    <li><a>@Html.ActionLink("Pagina Inicial", "Home", "Home")</a></li>
    <li>
        <a>Ordem de Serviço</a>
        <ul>
            <li><a> @Html.ActionLink("Cadastrar Ordem Serviço", "CadastroOrdemServico", "OrdemServico")</a></li>
            <li><a>@Html.ActionLink("Listar ordem Serviço", "ListarOrdemServico", "OrdemServico")</a></li>
        </ul>
    </li>
    <li>
        <a>Veículos</a>
        <ul>
            <li><a> @Html.ActionLink("Cadastar Veículo", "CadastroVeiculo", "Veiculo")</a></li>
            <li><a> @Html.ActionLink("Listar Veículos", "ListarVeiculos", "Veiculo")</a></li>
        </ul>
    </li>
    <li>
        <a>Clientes</a>
        <ul>
            <li><a> @Html.ActionLink("Cadastar Cliente", "CadastroPessoa", "Pessoa")</a></li>
            
        </ul>
    </li>
    <li>
        <a>Usuários</a>
        <ul>
            <li><a>Cadastrar Usuário</a></li>
            <li><a>Listar Usuários</a></li>
            <li><a>Deletar Usuário</a></li>
        </ul>
    </li>
</ul>
<br />
<br />
<br />
<br />
<br /><div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cpf_Cnpj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endereco)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SeletorPessoa)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cpf_Cnpj)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endereco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SeletorPessoa)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
    </div>
